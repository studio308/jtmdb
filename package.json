{
  "name": "tmdb-client",
  "version": "0.1.0",
  "description": "A javascript client library for The Movie Database api",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "tslint {src,test}/**/*.ts --format stylish",
    "test": "nyc --check-coverage --lines 100 mocha --reporter progress",
    "test:watch": "mocha -w --reporter min"
  },
  "nyc": {
    "require": [
      "ts-node/register"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/studio-308/tmdb-client-js"
  },
  "keywords": [
    "TMDB",
    "client"
  ],
  "author": "Studio 308 LLC",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/studio-308/tmdbs-js/issues"
  },
  "homepage": "https://gitlab.com/studio-308/tmdbs-js#readme",
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/chai-as-promised": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.35",
    "@types/proxyquire": "^1.3.28",
    "@types/sinon": "^9.0.11",
    "@types/sinon-chai": "^3.2.5",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^9.2.4",
    "sinon-chai": "^3.5.0",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "axios": "^0.21.1"
  }
}
