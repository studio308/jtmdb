{
  "name": "jtmdb",
  "version": "0.1.0",
  "description": "A javascript client library for The Movie Database api",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "/lib"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build && npm run test",
    "lint": "tslint {src,test}/**/*.ts --format stylish",
    "test": "jest --coverage"
  },
  "nyc": {
    "require": [
      "ts-node/register"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/studio308/jtmdb"
  },
  "keywords": [
    "TMDB",
    "client"
  ],
  "author": "Studio 308 LLC",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/studio308/jtmdb/issues"
  },
  "homepage": "https://github.com/studio308/jtmdb#readme",
  "devDependencies": {
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.35",
    "codecov": "^3.8.1",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "axios": "^0.21.1"
  }
}
